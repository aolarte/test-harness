<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>gae</application> <!-- unused for Cloud SDK based tooling, defined in pom.xml plugin config -->
    <version>2</version> <!-- unused for Cloud SDK based tooling, defined in pom.xml plugin config -->
    <runtime>java8</runtime>
    <threadsafe>true</threadsafe>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <automatic-scaling>
        <min-idle-instances>0</min-idle-instances>
    </automatic-scaling>
</appengine-web-app>
